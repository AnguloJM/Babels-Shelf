n    <div className=\"App\">\n      <Layout\n        currentUser={currentUser}\n        handleLogout={handleLogout}\n        currentPath={currentPath}\n      >\n        <Switch>\n          <Route exact path=\"/addBooks\" component={AddBooks} />\n          <Route exact path=\"/editBook/:id/edit\" component={EditBook} />\n          <Route exact path=\"/editbook/:id\" component={EditBook} />\n          \n          <Route exact path=\"/bookshelf\">\n            <BookShelf currentUser={currentUser}/>\n          </Route>\n          \n          <Route exact path='/myShelf'>\n            <MyShelf currentUser={currentUser}/>\n          </Route>\n            \n          <Route exact path='/login'>\n            <Login handleLogin={handleLogin} setCurrentPath={handlePath}/>\n          </Route>\n\n          <Route exact path='/register'>\n            <Register handleRegister={handleRegister} setCurrentPath={handlePath} />\n          </Route>\n\n          <Route exact path='/dashboard'>\n            <Dashboard currentUser={currentUser} />\n          </Route>\n\n          <Route exact path='/'>\n            <HomePage setCurrentPath={handlePath} />\n          </Route>\n          \n        </Switch>\n      </Layout>\n    </div>\n  );\n}\n\nexport default App;\n","/Users/jairoangulo/General_Assembly/sei/phoenix/unit_4/project-4/Babels-Shelf/client/src/screens/HomePage/HomePage.jsx",[],"/Users/jairoangulo/General_Assembly/sei/phoenix/unit_4/project-4/Babels-Shelf/client/src/components/shared/Layout/Layout.jsx",[],"/Users/jairoangulo/General_Assembly/sei/phoenix/unit_4/project-4/Babels-Shelf/client/src/components/shared/Header/Header.jsx",[],"/Users/jairoangulo/General_Assembly/sei/phoenix/unit_4/project-4/Babels-Shelf/client/src/components/shared/Footer/Footer.jsx",[],["82","83"],"/Users/jairoangulo/General_Assembly/sei/phoenix/unit_4/project-4/Babels-Shelf/client/src/screens/Login/login.jsx",[],"/Users/jairoangulo/General_Assembly/sei/phoenix/unit_4/project-4/Babels-Shelf/client/src/